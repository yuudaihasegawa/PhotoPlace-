<div class="container">
  <h2 class="text-center">会員一覧</h2>
    <% @user.each do |user| %>
      <div class="posts" >
        <%= link_to admin_user_path(user) do %>
          <%= attachment_image_tag user, :profile_image, size: "200x200", fallback: "no_image.jpg" %>
          <div class="text-center">
            <%= user.name %>
          </div>
        <% end %>
      </div>
    <% end %>
</div>